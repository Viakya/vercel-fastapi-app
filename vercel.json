{
  "version": 2,
  "builds": [
    { "src": "api/latency.py", "use": "@vercel/python" }
  ],
  "routes": [
    {
      "src": "/api/latency",
      "dest": "api/latency.py",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE,OPTIONS",
        "Access-Control-Allow-Headers": "*"
      }
    }
  ]
}
